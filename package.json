{
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "echo \"This template does not include a test runner by default.\" && exit 1",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "prettier --check \"src/**/*.{ts,js}\"",
    "assets": "npx http-server --cors -c-1 -p 8081 assets/",
    "serve": "npx http-server --cors -c-1 -p 8080 build/",
    "dev": "npm run build && npm run serve"
  },
  "dependencies": {
    "canvas-confetti": "^1.2.0",
    "video.js": "^5.20.2",
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "@snowpack/plugin-run-script": "^2.1.7",
    "@snowpack/plugin-typescript": "^1.0.2",
    "@types/canvas-confetti": "^1.0.0",
    "@types/jquery": "^3.5.4",
    "@types/snowpack-env": "^2.3.0",
    "@types/video.js": "^7.3.11",
    "http-server": "^0.12.3",
    "prettier": "^2.0.5",
    "snowpack": "^2.15.1",
    "typescript": "^4.0.0"
  }
}
